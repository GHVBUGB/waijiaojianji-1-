{
  "functions": {
    "api/index.py": { "runtime": "python3.11" }
  },
  "routes": [
    { "src": "/static/(.*)", "dest": "/app/static/$1" },
    { "src": "/uploads/(.*)", "dest": "/uploads/$1" },
    { "src": "/outputs/(.*)", "dest": "/outputs/$1" },
    { "src": "/(.*)", "dest": "/api/index.py" }
  ]
}
